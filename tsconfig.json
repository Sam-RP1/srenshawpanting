{
    "compilerOptions": {
      "noImplicitAny": false,
      "allowJs": true,
      "removeComments": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
		  "allowSyntheticDefaultImports": true,
		  "strict": true,
      "module": "esnext",
      "target": "es5",
      "resolveJsonModule": true,
		  "noEmit": true,
		  "jsx": "react"
    },
    "exclude": [
      "node_modules"
    ]
}